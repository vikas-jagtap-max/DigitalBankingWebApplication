##
#server.port=8762
#spring.application.name=api-gateway


##Registering Spring Cloud Gateway to Spring Cloud Consul server
#Spring.cloud.gateway.discovery.locator.enabled=true
#spring.cloud.consul.defaultZone: http://localhost:8500



##Congiguring Spring Cloud Gateway
#spring.cloud.gateway.routes.id=USER-MICROSERVICE
#spring.cloud.gateway.routes.uri=lb://USER-MICROSERVICE
#spring.cloud.gateway.routes.predicates.path=/infymemobile/**

